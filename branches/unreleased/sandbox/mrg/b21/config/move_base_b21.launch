<launch>
 
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <remap from="cmd_vel" to="b21/cmd_vel"/>
    <rosparam file="$(find b21)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find b21)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find b21)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find b21)/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find b21)/config/base_local_planner_params.yaml" command="load" />
  </node>


</launch>
